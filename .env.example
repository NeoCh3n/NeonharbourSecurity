############################################################
# Asia Agentic SOC - Environment Example (AWS-first)
# Copy to `.env` and fill in placeholders (do NOT commit secrets)
############################################################

# Core AWS wiring
AWS_REGION=ap-southeast-1
EVENT_BUS_NAME=AsiaAgenticSocBus
STATE_MACHINE_ARN=arn:aws:states:ap-southeast-1:123456789012:stateMachine:AsiaAgenticSocStateMachine
DDB_INVESTIGATIONS_TABLE=AsiaAgenticSocInvestigations-dev
DDB_METRICS_TABLE=AsiaAgenticSocMetrics-dev
ARTIFACTS_BUCKET=asia-agentic-soc-artifacts-123456789012-dev
AUDIT_BUCKET=asia-agentic-soc-audit-123456789012-dev
KMS_KEY_ID=arn:aws:kms:ap-southeast-1:123456789012:key/example
SECRETS_MANAGER_PATH=/asia-agentic-soc/dev/

# AI providers
AI_PROVIDER=bedrock # bedrock|kiro|amazonq
BEDROCK_REGION=ap-southeast-1
BEDROCK_TEXT_MODEL=anthropic.claude-3-haiku-20240307-v1:0
BEDROCK_EMBED_MODEL=amazon.titan-embed-text-v2
BEDROCK_MAX_TOKENS=2048
BEDROCK_TEMPERATURE=0.1
# Kiro & Amazon Q credentials should be stored in Secrets Manager and referenced here
KIRO_API_ENDPOINT=
KIRO_API_KEY=
AMAZONQ_WORKSPACE_ID=
AMAZONQ_AUTH_TOKEN=

# Connector credentials (read-only)
SENTINEL_WORKSPACE_ID=
SENTINEL_CLIENT_ID=
SENTINEL_CLIENT_SECRET=
AZURE_TENANT_ID=

SPLUNK_BASE_URL=https://splunk.example.com:8089
SPLUNK_USERNAME=
SPLUNK_PASSWORD=

DEFENDER_CLIENT_ID=
DEFENDER_CLIENT_SECRET=

CROWDSTRIKE_CLIENT_ID=
CROWDSTRIKE_CLIENT_SECRET=

ENTRA_CLIENT_ID=
ENTRA_CLIENT_SECRET=

OKTA_BASE_URL=https://tenant.okta.com
OKTA_API_TOKEN=

# Collaboration / Ticketing placeholders (Phase B)
SLACK_WEBHOOK_URL=
TEAMS_WEBHOOK_URL=
SERVICENOW_INSTANCE=
SERVICENOW_USER=
SERVICENOW_PASSWORD=
JIRA_BASE_URL=
JIRA_API_TOKEN=

# Rate limit knobs
CONNECTOR_RPS=5
CONNECTOR_BURST=10

# Demo & fixtures
CONNECTOR_FIXTURES=tools/seed
DEFAULT_TENANT_ID=hk-demo

# Streamlit UI demo token (use Secrets Manager in production)
DEMO_AUTH_TOKEN=change-me
